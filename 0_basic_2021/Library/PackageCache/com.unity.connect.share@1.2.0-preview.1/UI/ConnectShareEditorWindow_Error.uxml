<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements">
    <ui:VisualElement name="Header" class="header-container">
        <Style src="ConnectShareEditorWindow_Styles.uss" />
        <ui:Image name="imgThumbnail" class="header-thumbnail" />
        <ui:VisualElement name="ProjectInfo" class="header-project-info">
            <ui:Label text="Project Name" name="lblProjectName" class="header-project-name" />
            <ui:Label text="By email@example.com" name="lblUserEmail" class="header-user-email" />
        </ui:VisualElement>
    </ui:VisualElement>
    <ui:VisualElement name="MainWindow" class="main-container">
        <Style src="ConnectShareEditorWindow_Styles.uss" />
        <ui:VisualElement name="imgError" class="status-icon status-icon-error" />
        <ui:Label text="Something went wrong" name="lblTitle" class="progress-label" />
        <ui:Label text="Error goes here" name="lblError" class="error-label" />
    </ui:VisualElement>
    <ui:Button text="Go Back" name="btnBack" class="bottom-button">
        <Style src="ConnectShareEditorWindow_Styles.uss" />
    </ui:Button>
</ui:UXML>
